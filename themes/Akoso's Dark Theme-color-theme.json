{
	"name": "Akoso's Dark Theme",
	"colors": {


		// Editor
		"editor.background": "#1F1E1D",
		"editor.foreground":  "#F1ECE1",

		"editorLineNumber.foreground": "#F1ECE1",
		"editorLineNumber.activeForeground": "#C6AC8F",

		"editorCursor.background": "#DED0BE",
		"editorCursor.foreground": "#DED0BE",

		"editor.selectionBackground": "#70635475",
		"editor.inactiveSelectionBackground": "#706354",


		// General
		"foreground": "#EAE2D6",
		"focusBorder": "#706354",
		"progressBar.background": "#706354",
		"selection.background": "#4C6663",
		"scrollbar.shadow": "#4C6663",

		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",


		// Title Bar
		"titleBar.activeBackground": "#20373C",
		"titleBar.activeForeground": "#F1ECE180",
		"titleBar.border": "#93b6ba15",

		"titleBar.inactiveBackground": "#20373C",
		"titleBar.inactiveForeground": "#F1ECE180",


		// Menu Bar
		"menubar.selectionBackground": "#35535A",
		"menubar.selectionForeground": "#f1ece1d6",
		"menubar.selectionBorder": "#ff000000",

		"menu.background": "#35535A",
		"menu.foreground": "#f1ece1d6",

		"menu.selectionBackground": "#f1ece130",
		"menu.selectionForeground": "#f1ece1d6",
		"menu.selectionBorder": "#20373C",
		"menu.separatorBackground": "#6599A4",
		"menu.border": "#20373C",


		// Status Bar
		"statusBar.background": "#348092",
		"statusBar.foreground": "#F1ECE1",
		"statusBar.border": "#22333B",
		"statusBarItem.hoverBackground": "#DCE5E350",
		
		"statusBar.debuggingBackground": "#768937",
		"statusBar.debuggingForeground": "#F1ECE1",
		"statusBar.debuggingBorder": "#2B5353",

		"statusBar.noFolderBackground": "#504A8b",
		"statusBar.noFolderForeground": "#F1ECE1",
		"statusBar.noFolderBorder": "#373554",

		"statusBarItem.remoteBackground": "#2A4F57",
		"statusBarItem.remoteForeground": "#F1ECE1",


		// Buttons
		"button.background": "#348092",
		"button.foreground": "#f1ece1f5",
		"button.hoverBackground": "#2B6A78",

		"button.secondaryBackground": "#34819239",
		"button.secondaryForeground": "#f1ece1f5",
		"button.secondaryHoverBackground": "#ff0000",

		"textLink.foreground": "#348092",
		"textLink.activeForeground": "#93B6BA"
		
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
